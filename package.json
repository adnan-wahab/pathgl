{
  "name": "regl-network-vis",
  "version": "0.0.7",
  "description": "WebGL-based 2D Network Graph in Regl",
  "author": "Adnan Wahab",
  "keywords": [
    "network",
    "2d",
    "regl",
    "webgl"
  ],
  "license": "MIT",
  "main": "index.js",
  "module": "index.js",
  "files": [
    "dist/**/*.js",
    "src/*.js",
    "index.js"
  ],
  "scripts": {
    "build": "rollup -c; webpack --config ./webpack.config.js --mode production",
    "prepublishOnly": "npm run test; rm -rf dist/*; npm run build;",
    "prerelease": "rm -rf dist/*; npm run build; zip -r dist.zip dist",
    "start": "webpack-dev-server --config ./webpack.config.js --mode development --open",
    "test": "rollup -c ./rollup.test.config.js | tape-run --render='tap-spec'",
    "watch": "rollup -cw"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-transform-regenerator": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "acorn": "^6.0.4",
    "babel-loader": "^8.0.4",
    "babel-plugin-lodash": "^3.3.4",
    "browser-env": "^3.2.5",
    "esm": "^3.0.84",
    "gl": "^4.4.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.1.4",
    "merge": ">=1.2.1",
    "prettier": "^1.15.2",
    "pretty-quick": "^1.8.0",
    "rollup": "^0.67.3",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-filesize": "^5.0.1",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-terser": "^3.0.0",
    "rollup-plugin-visualizer": "^0.9.2",
    "tap-spec": "^5.0.0",
    "tape-run": "^6.0.1",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.9.0",
    "webpack-tape-run": "0.0.7",
    "zora": "^2.0.1"
  },
  "dependencies": {
    "angle-normals": "^1.0.0",
    "budo": "^9.4.7",
    "bunny": "^1.0.1",
    "canvas-fit": "^1.5.0",
    "canvas-orbit-camera": "^1.0.2",
    "d3": "^4.8.0",
    "gl-mat4": "^1.1.4",
    "glslify": "^6.0.2",
    "isnumber": "^1.0.0",
    "lodash": "^4.17.15",
    "mouse-change": "^1.4.0",
    "mouse-position": "^2.0.1",
    "mouse-pressed": "^1.0.0",
    "mouse-wheel": "^1.2.0",
    "pixel-grid": "^1.0.2",
    "regl": "^1.3.0",
    "regl-camera": "^2.1.1",
    "underscore": "^1.8.3"
  },
  "homepage": "https://github.com/adnan-wahab/regl-network-vis",
  "repository": {
    "type": "git",
    "url": "git://github.com/adnan-wahab/regl-network-vis.git"
  },
  "bugs": {
    "url": "https://github.com/adnan-wahab/regl-network-vis/issues"
  }
}
